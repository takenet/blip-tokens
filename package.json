{
  "name": "blip-tokens",
  "version": "0.0.0-development",
  "description": "",
  "type": "module",
  "main": "build/index.js",
  "exports": {
    ".": "./build/index.js",
    "./icons": {
      "types": "./build/icons.d.ts",
      "default": "./build/icons.js"
    }
  },
  "files": [
    "build",
    "properties"
  ],
  "scripts": {
    "build": "node_modules/.bin/style-dictionary build --config ./config.cjs",
    "clean": "rm -rf build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release",
    "optimize:svg": "svgo assets/**/*.svg --config=svgo.config.js --quiet",
    "optimize:svg:check": "svgo assets/**/*.svg --config=svgo.config.js --dry-run",
    "analyze:svg": "bash svg-analysis.sh",
    "check:svg:size": "bash scripts/check-svg-size.sh"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "semantic-release": "^21.0.2",
    "style-dictionary": "^2.8.3",
    "svgo": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takenet/blip-tokens.git"
  }
}
